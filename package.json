{
  "name": "gitlab-build-monitor",
  "version": "0.0.1",
  "description": "A simple web application which monitors build statuses and other relevant information about all ongoing COIN projects in GitLab.",
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "ng build",
    "build:prod": "npm run clean:build && ng build --prod --base-href /gitlab-build-monitor/ --output-path=build",
    "check:all": "npm run lint && npm run test",
    "clean:all": "npm run clean:build && npm run clean:node",
    "clean:build": "rm -rf coverage build",
    "clean:node": "rm -rf node_modules && npm cache clean --force && rm -f package-lock.json && npm install",
    "eslint": "eslint src e2e",
    "e2e": "ng e2e",
    "lint": "npm run eslint && npm run stylelint && npm run tslint -- --project ./tsconfig.json 'src/**/*.ts'",
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "ng serve --proxy-config ./proxy.conf.json",
    "start:hmr": "ng serve --proxy-config ./proxy.conf.json --configuration hmr",
    "start:prod": "ng serve --configuration prod",
    "stylelint": "stylelint '**/*.scss'",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test --watch=true",
    "test-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessCustom",
    "tslint": "tslint",
    "yarn": "yarn"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.1.0",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "@types/chart.js": "^2.8.3",
    "chart.js": "^2.8.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "yarn": "^1.17.3"
  }
}
